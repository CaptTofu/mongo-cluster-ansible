- hosts: "{{ target|default('localhost') }}"
  tasks:
  - name: delete 3 instances
    ec2:
      state: absent
      region: us-west-1
      instance_ids:
{% for item in ec2_info_one.instances %}
      - {{ item.id }}
{% endfor %}

